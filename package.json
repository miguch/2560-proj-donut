{
  "name": "course_schedule_system",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently --kill-others \"npm run front\" \"node server/server.js\"",
    "server": "nodemon -r dotenv/config --watch server server/server.js",
    "front": "vite",
    "dev": "NODE_ENV=development concurrently --kill-others \"npm run front\" \"nodemon -r dotenv/config --watch server server/server.js\""
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "^2.1.0",
    "animate.css": "^4.1",
    "axios": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "element-ui": "^2.15",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "glob": "^8.0.3",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^5.2.4",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-github": "^1.1.0",
    "vite": "^3.2.4",
    "vue": "^2.7",
    "vue-router": "^3.5",
    "vuex": "^3.6"
  },
  "license": "MIT"
}
